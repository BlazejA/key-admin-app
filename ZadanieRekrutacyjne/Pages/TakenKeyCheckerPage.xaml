<UserControl x:Class="ZadanieRekrutacyjne.TakenKeyCheckerPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ZadanieRekrutacyjne"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <TextBlock Text="Pobrane klucze:"/>
        <StackPanel>
            <ItemsControl ItemsSource="{Binding TakenKeyChecker}">
                <ItemsControl.ItemTemplate>
                    <DataTemplate>
                        <local:TakenKeyChecker />
                    </DataTemplate>
                </ItemsControl.ItemTemplate>
            </ItemsControl>
        </StackPanel>
    </Grid>
</UserControl>
