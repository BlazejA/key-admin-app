<Application x:Class="ZadanieRekrutacyjne.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ZadanieRekrutacyjne"
             xmlns:viewmodels="clr-namespace:ZadanieRekrutacyjne.Core;assembly=ZadanieRekrutacyjne.Core"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <DataTemplate DataType="{x:Type viewmodels:KeysListPageViewModel}">
            <local:KeysListPage />
        </DataTemplate>
        <DataTemplate DataType="{x:Type viewmodels:TakenKeyCheckerPageViewModel}">
            <local:TakenKeyCheckerPage />
        </DataTemplate>
        <DataTemplate DataType="{x:Type viewmodels:EmployeesListPageViewModel}">
            <local:EmployeesListPage />
        </DataTemplate>
    </Application.Resources>
</Application>
